<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>TensorFlowSharp</PackageId>
    <PackageVersion>1.4.0</PackageVersion>
    <Authors>Miguel de Icaza</Authors>
    <PackageLicenseUrl>https://github.com/migueldeicaza/TensorFlowSharp/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/migueldeicaza/TensorFlowSharp/</PackageProjectUrl>
    <PackageTags>machine-learning, tensorflow, xamarin, c#, f#</PackageTags>
    <Description>.NET Bindings for TensorFlow</Description>
    <Owners>Miguel de Icaza</Owners>
    <Summary>.NET API for TensorFlow, Google's Machine Intelligence framework</Summary>
    <PackageReleaseNotes>Adds support for TensorFlow 1.4

This brings support for the TensorFlow 1.4 API.</PackageReleaseNotes>
  </PropertyGroup>
  <ItemGroup>
    <_NativeFiles Include="..\native\*.*">
      <InProject>false</InProject>
    </_NativeFiles>
    <Content Include="@(_NativeFiles)">
      <Kind>None</Kind>
      <PackagePath>native\%(Filename)%(Extension)</PackagePath>
    </Content>
    <Content Include="TensorFlowSharp.targets">
      <PackagePath>build\TensorFlowSharp.targets</PackagePath>
    </Content>
    <Content Include="..\native\android\armeabi-v7a\*.*" Exclude="..\native\android\armeabi-v7a\.gitkeep">
      <Kind>None</Kind>
      <PackagePath>native\android\armeabi-v7a\%(Filename)%(Extension)</PackagePath>
    </Content>
    <Content Include="..\native\android\arm64-v8a\*.*" Exclude="..\native\android\arm64-v8a\.gitkeep">
      <Kind>None</Kind>
      <PackagePath>native\android\arm64-v8a\%(Filename)%(Extension)</PackagePath>
    </Content>
    <Content Include="Android.targets">
      <PackagePath>build\MonoAndroid10\TensorFlowSharp.targets</PackagePath>
    </Content>
  </ItemGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
    <OutputPath>bin\Debug</OutputPath>
    <DefineConstants>DEBUG</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
    <DocumentationFile>bin\Debug\TensorFlowSharp.xml</DocumentationFile>
    <ConsolePause>false</ConsolePause>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <Optimize>true</Optimize>
    <OutputPath>bin\Release</OutputPath>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
    <ConsolePause>false</ConsolePause>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>
</Project>